{
  "name": "my-porquinho",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "swc src -d build",
    "build-dev": "npm run build && NODE_ENV=development node --experimental-specifier-resolution=node --inspect build/server.js",
    "dev:swc": "nodemon --ext ts --exec \"npm run build-dev\"",
    "test": "NODE_ENV=test && mocha --require ts-node/register __tests__/**/**/**/**.spec.ts",
    "test:unit": "NODE_ENV=test && mocha --require ts-node/register __tests__/**/**/Unit/**/**.spec.ts",
    "test:int": "export NODE_ENV=test && npm run migration:run && mocha --require ts-node/register __tests__/**/**/Integration/**.test.ts -t 10000 --exit",
    "test:nyc": "nyc npm run test",
    "dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
    "lint": "eslint .",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/app/infra/typeorm/data-source.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TiagoPaz2000/My-Porquinho.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TiagoPaz2000/My-Porquinho/issues"
  },
  "homepage": "https://github.com/TiagoPaz2000/My-Porquinho#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.207",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.45",
    "@types/sinon": "^10.0.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "chai": "^4.3.6",
    "dotenv": "^16.0.1",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-typescript": "^16.1.3",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.18",
    "nyc": "^15.1.0",
    "sinon": "^13.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.6"
  }
}
